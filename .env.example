# YouTube
YOUTUBE_API_KEY=

# Notifications (Discord Incoming Webhook)
DISCORD_WEBHOOK_URL=

# Web app
APP_PORT=8001
DATABASE_URL=postgresql://elt_user:elt_pass@postgres:5432/elt_db

# Optional: Override the ELT Postgres connection Airflow uses (e.g., Supabase).
# If set, Airflow will read/write YouTube data to that DB instead of the local docker Postgres.
# AIRFLOW_CONN_POSTGRES_DB_YT_ELT=postgresql://user:pass@host:5432/dbname

# Airflow
AIRFLOW_WWW_USER_USERNAME=airflow
AIRFLOW_WWW_USER_PASSWORD=airflow
FERNET_KEY=
AIRFLOW_UID=50000

# Docker image name used by docker-compose.yaml
DOCKERHUB_NAMESPACE=local
DOCKERHUB_REPOSITORY=yt_elt

# Container-to-container Postgres connection
POSTGRES_CONN_HOST=postgres
POSTGRES_CONN_PORT=5432

# Postgres superuser for the container itself
POSTGRES_CONN_USERNAME=postgres
POSTGRES_CONN_PASSWORD=postgres

# Airflow metadata DB
METADATA_DATABASE_NAME=airflow_meta
METADATA_DATABASE_USERNAME=airflow
METADATA_DATABASE_PASSWORD=airflow

# Celery result backend DB
CELERY_BACKEND_NAME=airflow_celery
CELERY_BACKEND_USERNAME=airflow
CELERY_BACKEND_PASSWORD=airflow

# ELT DB where staging/core schemas live
ELT_DATABASE_NAME=elt_db
ELT_DATABASE_USERNAME=elt_user
ELT_DATABASE_PASSWORD=elt_pass
